{
  "name": "krypton-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.11",
    "@sentry/browser": "^4.5.3",
    "@testing-library/react": "^8.0.1",
    "antd": "^3.15.1",
    "colors": "^1.3.3",
    "downloadjs": "^1.4.7",
    "emotion": "^10.0.9",
    "jest-dom": "^3.4.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "node-less-chokidar": "^0.1.2",
    "npm-run-all": "^4.1.3",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.2",
    "query-string": "5.0.0",
    "react": "^16.8.6",
    "react-collapsible": "^2.6.0",
    "react-dom": "^16.8.6",
    "react-dropzone": "^4.2.9",
    "react-infinite-scroller": "^1.2.0",
    "react-intl": "^2.7.2",
    "react-moment": "^0.9.2",
    "react-number-format": "^4.2.0",
    "react-recaptcha-v3-global": "^2.1.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "3.0.1",
    "react-telephone-input": "^4.71.11",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "validator": "^9.4.0"
  },
  "scripts": {
    "build-css": "node-less-chokidar src/ -o src/",
    "watch-css": "node-less-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "validate-env": "node -r dotenv/config src/js/utils/validate-env.js",
    "start": "npm run validate-env && npm run build-css && run-p -ncr watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm run validate-env && npm-run-all build-css build-js",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "rwstart": "react-app-rewired start",
    "rwbuild": "react-app-rewired build",
    "rwtest": "react-app-rewired test --env=jsdom"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "react-app-rewired": "^2.1.3",
    "webpack-deadcode-plugin": "^0.1.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,td,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": {
    "name": "Rajat Vijay",
    "email": "rajatvijay5@gamil.com"
  }
}
