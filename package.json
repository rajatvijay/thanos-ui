  "scripts": {
    "build-css": "node-less-chokidar src/ -o src/",
    "watch-css": "node-less-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "validate-env": "node -r dotenv/config src/js/utils/validate-env.js",
    "start": "npm run validate-env && npm run build-css && run-p -ncr watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm run validate-env && npm-run-all build-css build-js",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "husky": "^2.3.0",
